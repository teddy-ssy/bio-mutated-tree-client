<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shengyuansun.serendipitiosgene">

    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/geneicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/geneicon"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectFileActivity"/>
        <activity android:name=".VisualizationExcelActivity"/>
        <activity android:name=".VisualizationCircleActivity"></activity>
        <activity android:name=".CircleActivity"/>
        <activity android:name=".CloudViewActivity"></activity>
        <activity android:name=".CloudViewNewActivity"></activity>
        <activity android:name=".CloudUploadActivity"></activity>
        <activity android:name=".ResistancegeneModifyActivity"></activity>
    </application>


</manifest>