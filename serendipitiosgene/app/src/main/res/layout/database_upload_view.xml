<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="20dp"
    android:paddingRight="20dp">

    <TextView
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_database_upload_1"
        android:text="version:"/>
    <TextView
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3.1415"
        android:layout_alignBaseline="@+id/textview_database_upload_1"
        android:layout_alignBottom="@+id/textview_database_upload_1"
        android:layout_toEndOf="@+id/textview_database_upload_1" />

    <TextView
        android:layout_below="@+id/textview_database_upload_1"
        android:id="@+id/textview_database_upload_2"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bindingprotein:"/>

    <EditText
        android:id="@+id/EditText_database_add_bindingprotein"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textview_database_upload_2"
        android:layout_toEndOf="@+id/textview_database_upload_2"/>

    <TextView
        android:layout_below="@+id/textview_database_upload_2"
        android:id="@+id/textview_database_upload_4"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="position:"/>

    <EditText
        android:id="@+id/EditText_database_add_position"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textview_database_upload_4"
        android:layout_toEndOf="@+id/textview_database_upload_4"/>

    <TextView
        android:layout_below="@+id/textview_database_upload_4"
        android:id="@+id/textview_database_upload_5"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="orginal gene:"/>

    <EditText
        android:id="@+id/EditText_database_add_orginalcode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textview_database_upload_5"
        android:layout_toEndOf="@+id/textview_database_upload_5"/>

    <TextView
        android:layout_marginTop="20dp"
        android:layout_below="@+id/textview_database_upload_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_database_upload_3"
        android:text="my data:"/>
    <Button
        android:background="@drawable/insert"
        android:layout_width="40dp"
        android:layout_height="40dp"

        android:layout_alignParentBottom="true"
        android:id="@+id/button_database_upload"
        android:layout_centerHorizontal="true"/>

    <ListView
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:id="@+id/listview_database_upload"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/textview_database_upload_3"
        android:layout_above="@+id/button_database_upload"></ListView>

</RelativeLayout>